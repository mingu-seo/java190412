<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="admin">
    
    <typeAlias alias="vo" type="manage.member.MemberVO"/>
    
     <select id="loginCheck" parameterClass="vo" resultClass="Integer">
        select count(*) as cnt from member where id=#id# and password=password(#password#)
    </select>













</sqlMap>
